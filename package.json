    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "csv-parser": "^3.0.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "install": "^0.13.0",
    "next": "13.4.4",
    "npm": "^9.8.1",
    "postcss": "8.4.24",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
